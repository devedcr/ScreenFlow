                        </VBox>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <VBox alignment="TOP_CENTER" prefHeight="73.0" prefWidth="380.0" spacing="5.0">
                            <children>
                                <HBox prefHeight="28.0" prefWidth="249.0" spacing="10.0">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-color"
                                              text="BitRate Mbps"/>
                                        <TextField alignment="CENTER" prefHeight="18.0" prefWidth="35.0">
                                            <font>
                                                <Font size="9.0"/>
                                            </font>
                                        </TextField>
                                    </children>
                                </HBox>
                                <Slider prefHeight="16.0" prefWidth="318.0"/>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>

</AnchorPane>
