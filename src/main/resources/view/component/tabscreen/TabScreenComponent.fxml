                                            <image>
                                                <Image url="@../../../image/monitor.png"/>
                                            </image>
                                        </ImageView>
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER" prefHeight="105.0" prefWidth="186.0">
                                    <children>
                                        <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0"
                                              prefWidth="170.0" spacing="5.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-color"
                                                      text="Resolution"/>
                                                <ComboBox prefHeight="24.0" prefWidth="169.0"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </VBox>
                                    </children>
                                </VBox>
                            </children>
                        </VBox>
                        <VBox alignment="TOP_CENTER" prefHeight="212.0" prefWidth="212.0" spacing="30.0">
